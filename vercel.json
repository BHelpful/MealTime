{
	"version": 2,
	"builds": [
		{
			"src": "apps/api/package.json",
			"use": "@vercel/node"
		},
		{
			"src": "apps/web/package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist/apps/web"
			}
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/$1"
		}
	]
}

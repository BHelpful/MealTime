{
	"name": "meal-time",
	"version": "0.0.0",
	"license": "MIT",
	"scripts": {
		"ng": "nx",
		"prestart": "yarn prisma:seed",
		"start": "nx run-many --target=serve --projects=api,meal-time",
		"start:clean": "yarn db:reset && yarn start",
		"build": "nx affected --target=build --base=origin/master --parallel",
		"build:meal-time": "nx build meal-time",
		"build:api": "nx build api",
		"lint": "nx affected --target=lint --base=origin/master --parallel",
		"test": "nx affected --target=test --base=origin/master --parallel",
		"test:cov": "nx affected --target=test --base=origin/master --parallel --codeCoverage",
		"e2e": "yarn e2e:script -- 'nx affected:e2e --base=origin/master --parallel'",
		"e2e:watch": "yarn e2e:script -- 'nx affected:e2e --base=origin/master --parallel --watch'",
		"e2e:script": "start-server-and-test \"nx serve api\" http://localhost:3333/api",
		"db:start": "docker-compose up -d",
		"db:stop": "docker-compose down",
		"db:push": "prisma db push",
		"db:reset": "yarn db:stop && yarn db:start && yarn db:push",
		"prisma:generate": "prisma generate",
		"prisma:studio": "prisma studio",
		"prisma:seed": "prisma db seed",
		"api:res": "nx g @nrwl/nest:resource --project=api",
		"api:s": "nx g @nrwl/nest:service --project=api",
		"api:co": "nx g @nrwl/nest:controller --project=api",
		"api:d": "nx g @nrwl/nest:decorator --project=api",
		"api:mi": "nx g @nrwl/nest:middleware --project=api",
		"api:f": "nx g @nrwl/nest:filter --project=api",
		"api:gu": "nx g @nrwl/nest:guard --project=api",
		"api:mod": "nx g @nrwl/nest:module --project=api",
		"front:co": "nx g @nrwl/angular:component --project=meal-time",
		"front:mod": "nx g @nrwl/angular:module --project=meal-time --routing",
		"ui": "nx run mealtime-ui-components:storybook",
		"ui:component": "nx g @nrwl/angular:component --project=mealtime-ui-components --export && yarn ui:stories",
		"ui:stories": "nx g @nrwl/angular:stories --project=mealtime-ui-components --generateCypressSpecs=false",
		"ui:tokens": "yarn style-dictionary build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "15.0.3",
		"@angular/common": "15.0.3",
		"@angular/compiler": "15.0.3",
		"@angular/core": "15.0.3",
		"@angular/forms": "15.0.3",
		"@angular/platform-browser": "15.0.3",
		"@angular/platform-browser-dynamic": "15.0.3",
		"@angular/router": "15.0.3",
		"@angular/service-worker": "15.0.3",
		"@fortawesome/angular-fontawesome": "^0.11.1",
		"@fortawesome/fontawesome-svg-core": "^6.2.0",
		"@fortawesome/free-solid-svg-icons": "^6.2.0",
		"@nestjs/common": "9.0.0",
		"@nestjs/config": "^2.2.0",
		"@nestjs/core": "9.0.0",
		"@nestjs/jwt": "^10.0.2",
		"@nestjs/mapped-types": "*",
		"@nestjs/passport": "^9.0.3",
		"@nestjs/platform-express": "9.0.0",
		"@nestjs/swagger": "6.0.0",
		"@nrwl/angular": "15.3.0",
		"@prisma/client": "^4.3.1",
		"bcrypt": "^5.1.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"dotenv": "^16.0.1",
		"dotenv-cli": "^6.0.0",
		"passport-jwt": "^4.0.1",
		"passport-local": "^1.0.0",
		"reflect-metadata": "^0.1.13",
		"rxjs": "~7.5.0",
		"tslib": "^2.0.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "15.0.3",
		"@angular-eslint/eslint-plugin": "15.0.0",
		"@angular-eslint/eslint-plugin-template": "15.0.0",
		"@angular-eslint/template-parser": "15.0.0",
		"@angular/cli": "~15.0.0",
		"@angular/compiler-cli": "15.0.3",
		"@angular/language-service": "15.0.3",
		"@angular/pwa": "^14.2.5",
		"@nestjs/schematics": "9.0.0",
		"@nestjs/testing": "9.1.4",
		"@nrwl/cli": "15.3.0",
		"@nrwl/cypress": "15.3.0",
		"@nrwl/devkit": "15.3.0",
		"@nrwl/eslint-plugin-nx": "15.3.0",
		"@nrwl/jest": "15.3.0",
		"@nrwl/linter": "15.3.0",
		"@nrwl/nest": "15.3.0",
		"@nrwl/node": "15.3.0",
		"@nrwl/nx-cloud": "15.0.2",
		"@nrwl/storybook": "15.3.0",
		"@nrwl/workspace": "15.3.0",
		"@storybook/addon-essentials": "~6.5.12",
		"@storybook/angular": "~6.5.9",
		"@storybook/builder-webpack5": "~6.5.9",
		"@storybook/core-server": "~6.5.9",
		"@storybook/manager-webpack5": "~6.5.9",
		"@types/bcrypt": "^5.0.0",
		"@types/jest": "28.1.8",
		"@types/node": "18.7.1",
		"@types/passport-jwt": "^3.0.8",
		"@types/passport-local": "^1.0.35",
		"@typescript-eslint/eslint-plugin": "^5.54.0",
		"@typescript-eslint/parser": "^5.54.0",
		"ajv-formats": "^2.1.1",
		"cypress": "^11.0.0",
		"eslint": "~8.25.0",
		"eslint-config-prettier": "8.1.0",
		"eslint-plugin-cypress": "^2.10.3",
		"jest": "28.1.3",
		"jest-environment-jsdom": "28.1.1",
		"jest-preset-angular": "12.2.3",
		"ng-packagr": "15.0.3",
		"nx": "15.3.0",
		"pactum": "^3.1.13",
		"passport": "^0.6.0",
		"postcss": "^8.4.17",
		"postcss-import": "~14.1.0",
		"postcss-preset-env": "~7.5.0",
		"postcss-url": "~10.1.3",
		"prettier": "^2.6.2",
		"prisma": "^4.3.1",
		"start-server-and-test": "^1.14.0",

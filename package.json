{
  "name": "meal-time",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx run-many --target=serve --projects=api,meal-time",
    "build": "nx run-many --target=build --projects=api,meal-time",
    "test": "nx run-many --target=test --projects=api,meal-time",
    "api:res": "nx g @nrwl/nest:resource --project=api",
    "api:ser": "nx g @nrwl/nest:service --project=api",
    "api:con": "nx g @nrwl/nest:controller --project=api",
    "api:mod": "nx g @nrwl/nest:module --project=api",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "db:dev:rm": "docker compose rm dev-db -s -f -v",
    "db:dev:up": "docker compose up dev-db -d",
    "prisma:dev:deploy": "prisma migrate deploy",
    "db:dev:restart": "yarn db:dev:rm && yarn db:dev:up && sleep 10 && yarn prisma:dev:deploy",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "db:test:rm": "docker compose rm test-db -s -f -v",
    "db:test:up": "docker compose up test-db -d",
    "db:test:restart": "yarn db:test:rm && yarn db:test:up && sleep 10 && yarn prisma:test:deploy",
    "pretest:integration": "yarn db:test:restart",
    "test:integration": "dotenv -e .env.test -- jest --watch --no-cache --config ./apps/api/test/jest-integration.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.0.0",
    "@angular/common": "~14.0.0",
    "@angular/compiler": "~14.0.0",
    "@angular/core": "~14.0.0",
    "@angular/forms": "~14.0.0",
    "@angular/platform-browser": "~14.0.0",
    "@angular/platform-browser-dynamic": "~14.0.0",
    "@angular/router": "~14.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^6.0.3",
    "@nrwl/angular": "14.4.2",
    "@prisma/client": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv-cli": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~14.0.0",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "~14.0.0",
    "@angular/compiler-cli": "~14.0.0",
    "@angular/language-service": "~14.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/cli": "14.4.2",
    "@nrwl/cypress": "14.4.2",
    "@nrwl/eslint-plugin-nx": "14.4.2",
    "@nrwl/jest": "14.4.2",
    "@nrwl/linter": "14.4.2",
    "@nrwl/nest": "14.4.2",
    "@nrwl/node": "14.4.2",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "14.4.2",
    "@types/jest": "^28.1.6",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cypress": "^9.1.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "nx": "14.4.2",
    "pactum": "^3.1.13",
    "prettier": "^2.6.2",
    "prisma": "^4.0.0",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}

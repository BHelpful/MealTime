{
  "name": "meal-time",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "prestart": "yarn prisma:seed",
    "start": "nx run-many --target=serve --projects=api,meal-time",
    "build": "nx affected --target=build --base=origin/master --parallel",
    "lint": "nx affected --target=lint --base=origin/master --parallel",
    "test": "nx affected --target=test --base=origin/master --parallel",
    "test:cov": "nx affected --target=test --base=origin/master --parallel --codeCoverage",
    "e2e": "yarn e2e:script -- 'nx affected:e2e --base=origin/master --parallel'",
    "e2e:watch": "yarn e2e:script -- 'nx affected:e2e --base=origin/master --parallel --watch'",
    "e2e:script": "start-server-and-test \"nx serve api\" http://localhost:3333/api",
    "docker:prisma:deploy": "dotenv -e .env.docker -- prisma migrate deploy",
    "docker:start": " dotenv -e .env.docker -- nx run-many --target=serve --projects=api,meal-time --host 0.0.0.0",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "api:res": "nx g @nrwl/nest:resource --project=api",
    "api:s": "nx g @nrwl/nest:service --project=api",
    "api:co": "nx g @nrwl/nest:controller --project=api",
    "api:d": "nx g @nrwl/nest:decorator --project=api",
    "api:mi": "nx g @nrwl/nest:middleware --project=api",
    "api:f": "nx g @nrwl/nest:filter --project=api",
    "api:gu": "nx g @nrwl/nest:guard --project=api",
    "api:mod": "nx g @nrwl/nest:module --project=api",
    "ui:storybook": "nx run mealtime-ui-components:storybook",
    "ui:component": "nx g @nrwl/angular:component --project=mealtime-ui-components --export && yarn ui:stories",
    "ui:stories": "nx g @nrwl/angular:stories --project=mealtime-ui-components --generateCypressSpecs=false",
    "ui:tokens": "yarn style-dictionary build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gen:env": "nx workspace-generator gen-env"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.1.0",
    "@angular/common": "14.1.0",
    "@angular/compiler": "14.1.0",
    "@angular/core": "14.1.0",
    "@angular/forms": "14.1.0",
    "@angular/platform-browser": "14.1.0",
    "@angular/platform-browser-dynamic": "14.1.0",
    "@angular/router": "14.1.0",
    "@angular/service-worker": "14.1.0",
    "@nestjs/common": "9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/swagger": "6.0.0",
    "@nrwl/angular": "14.5.0",
    "@prisma/client": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "supertokens-node": "^11.0.1",
    "supertokens-web-js": "^0.1.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.0",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "14.1.0",
    "@angular/compiler-cli": "14.1.0",
    "@angular/language-service": "14.1.0",
    "@angular/pwa": "^14.1.0",
    "@nestjs/schematics": "9.0.0",
    "@nestjs/testing": "9.0.0",
    "@nrwl/cli": "14.5.0",
    "@nrwl/cypress": "14.5.0",
    "@nrwl/devkit": "14.5.0",
    "@nrwl/eslint-plugin-nx": "14.5.0",
    "@nrwl/jest": "14.5.0",
    "@nrwl/linter": "14.5.0",
    "@nrwl/nest": "14.5.0",
    "@nrwl/node": "14.5.0",
    "@nrwl/nx-cloud": "14.2.0",
    "@nrwl/storybook": "^14.5.8",
    "@nrwl/workspace": "14.5.0",
    "@storybook/addon-essentials": "~6.5.9",
    "@storybook/angular": "~6.5.9",
    "@storybook/builder-webpack5": "~6.5.9",
    "@storybook/core-server": "~6.5.9",
    "@storybook/manager-webpack5": "~6.5.9",
    "@types/jest": "^28.1.6",
    "@types/node": "18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "ng-packagr": "~14.1.0",
    "nx": "14.5.0",
    "pactum": "^3.1.13",
    "postcss": "^8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "^2.6.2",
    "prisma": "^4.0.0",
    "start-server-and-test": "^1.14.0",
    "style-dictionary": "^3.7.1",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2",
    "webpack": "^5.64.0"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}

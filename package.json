{
  "name": "meal-time",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "prestart": "yarn db:restart && yarn prisma:seed",
    "start": "nx run-many --target=serve --projects=api,meal-time",
    "build": "nx affected --target=build --base=origin/master --parallel",
    "lint": "nx affected --target=lint --base=origin/master --parallel",
    "e2e": "yarn e2e:script -- 'nx affected:e2e'",
    "e2e:watch": "yarn e2e:script -- 'nx affected:e2e --watch'",
    "e2e:script": "start-server-and-test \"yarn db:restart && yarn prisma:seed && nx run api:serve\" http://localhost:3333/api",
    "db:rm": "docker compose rm dev-db supertokens -s -f -v",
    "db:up": "docker compose --env-file ./.env up dev-db supertokens -d",
    "db:restart": "yarn db:rm && yarn db:up && yarn prisma:deploy ",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "prisma db seed",
    "pretest": "yarn test:db:restart",
    "test": "nx affected --target=test --base=origin/master --parallel",
    "test:cov": "nx affected --target=test --base=origin/master --parallel --codeCoverage",
    "test:db:rm": "docker compose rm test-db -s -f -v",
    "test:db:up": "docker compose --env-file ./.env.test up test-db -d",
    "test:prisma:deploy": "dotenv -e .env.test -- prisma migrate deploy",
    "test:db:restart": "yarn test:db:rm && yarn test:db:up && yarn test:prisma:deploy",
    "preci:test": "yarn ci:db:restart",
    "ci:test": "nx affected --target=test ---base=origin/master --parallel --codeCoverage",
    "ci:db:rm": "docker-compose rm -s -f -v test-db",
    "ci:db:up": "docker-compose up -d test-db",
    "ci:prisma:deploy": "prisma migrate deploy",
    "ci:db:restart": "yarn ci:db:rm && yarn ci:db:up && prisma generate && yarn ci:prisma:deploy",
    "ci:e2e": "yarn e2e:script -- 'nx affected:e2e'",
    "ci:e2e:watch": "yarn e2e:script -- 'nx affected:e2e --watch'",
    "ci:e2e:script": "start-server-and-test \"yarn ci:db:restart && yarn prisma:seed && nx run api:serve\" http://localhost:3333/api",
    "api:res": "nx g @nrwl/nest:resource --project=api",
    "api:s": "nx g @nrwl/nest:service --project=api",
    "api:co": "nx g @nrwl/nest:controller --project=api",
    "api:d": "nx g @nrwl/nest:decorator --project=api",
    "api:mi": "nx g @nrwl/nest:middleware --project=api",
    "api:f": "nx g @nrwl/nest:filter --project=api",
    "api:gu": "nx g @nrwl/nest:guard --project=api",
    "api:mod": "nx g @nrwl/nest:module --project=api",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "gen:env": "nx workspace-generator gen-env"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.1.0",
    "@angular/common": "14.1.0",
    "@angular/compiler": "14.1.0",
    "@angular/core": "14.1.0",
    "@angular/forms": "14.1.0",
    "@angular/platform-browser": "14.1.0",
    "@angular/platform-browser-dynamic": "14.1.0",
    "@angular/router": "14.1.0",
    "@angular/service-worker": "14.1.0",
    "@nestjs/common": "9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/swagger": "6.0.0",
    "@nrwl/angular": "14.5.0",
    "@prisma/client": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv-cli": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.4.0",
    "supertokens-node": "^11.0.1",
    "supertokens-web-js": "^0.1.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.0",
    "@angular-eslint/eslint-plugin": "~14.0.0",
    "@angular-eslint/eslint-plugin-template": "~14.0.0",
    "@angular-eslint/template-parser": "~14.0.0",
    "@angular/cli": "14.1.0",
    "@angular/compiler-cli": "14.1.0",
    "@angular/language-service": "14.1.0",
    "@angular/pwa": "^14.1.0",
    "@nestjs/schematics": "9.0.0",
    "@nestjs/testing": "9.0.0",
    "@nrwl/cli": "14.5.0",
    "@nrwl/cypress": "14.5.0",
    "@nrwl/devkit": "14.5.0",
    "@nrwl/eslint-plugin-nx": "14.5.0",
    "@nrwl/jest": "14.5.0",
    "@nrwl/linter": "14.5.0",
    "@nrwl/nest": "14.5.0",
    "@nrwl/node": "14.5.0",
    "@nrwl/nx-cloud": "14.2.0",
    "@nrwl/workspace": "14.5.0",
    "@types/jest": "^28.1.6",
    "@types/node": "18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "cypress": "^10.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "nx": "14.5.0",
    "pactum": "^3.1.13",
    "prettier": "^2.6.2",
    "prisma": "^4.0.0",
    "start-server-and-test": "^1.14.0",
    "ts-jest": "27.1.4",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
